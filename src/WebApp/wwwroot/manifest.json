{
  "_version": "1.12.0",
  "sap.app": {
    "id": "controleDeFuncionarios",
    "type": "application",
    "i18n": "i18n/i18n.properties",
    "title": "{{titulo}}",
    "description": "{{Descricao}}",
    "applicationVersion": {
      "version": "1.0.0"
    }
  },
  "sap.ui": {
    "technology": "UI5",
    "deviceTypes": {
      "desktop": true,
      "tablet": true,
      "phone": true
    }
  },

  "sap.ui5": {
    "rootView": {
      "viewName": "controleDeFuncionarios.view.app",
      "type": "XML",
      "id": "app"
    },

    "dependencies": {
      "minUI5Version": "1.93",
      "libs": {
        "sap.ui.core": {},
        "sap.m": {}
      }
    },
    "models": {
      "i18n": {
        "type": "sap.ui.model.resource.ResourceModel",
        "settings": {
          "bundleName": "controleDeFuncionarios.i18n.i18n",
          "supportedLocales": [""],
          "fallbackLocale": ""
        }
      },

      "invoice": {
        "type": "sap.ui.model.json.JSONModel",
        "uri": "Invoices.json"
      }
    },

    "routing": {
      "config": {
        "routerClass": "sap.m.routing.Router",
        "type": "View",
        "viewType": "XML",
        "path": "controleDeFuncionarios.view",
        "controlId": "list",
        "controlAggregation": "pages"
      },
      "routes": [
        {
          "pattern": "",
          "name": "listaTelaInicial",
          "target": "ListaTelaInicial"
        },
        {
          "pattern": "detalhesFuncionario/{id}",
          "name": "detalhesFuncionario",
          "target": "DetalhesFuncionario"
        }, {
          "pattern": "cadastroDeFuncionarios",
          "name": "cadastroDeFuncionarios",
          "target": "cadastroDeFuncionarios"
        },
        {
          "pattern": "cadastroDeFuncionarios/{id}",
          "name": "edicao",
          "target": "cadastroDeFuncionarios"
      }
      ],
      "targets": {
        "ListaTelaInicial": {
          "id": "listaTelaInicial",
          "name": "listaTelaInicial"
        },
        "DetalhesFuncionario": {
          "id": "detalhesFuncionario",
          "name": "detalhesFuncionario"
        },
        "cadastroDeFuncionarios": {
          "id": "cadastroDeFuncionarios",
          "name": "cadastroDeFuncionarios"
        },
        "cadastro": {
            "id": "cadastro",
            "name": "Cadastro"
        }
      }
    }
  }
}
